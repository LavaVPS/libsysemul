#!/usr/bin/make -f

export DPKG_EXPORT_BUILDFLAGS = 1
include /usr/share/dpkg/buildflags.mk

%:
	dh $@

override_dh_installdeb: debian/libsysemul0.postinst debian/libsysemul0.prerm
	dh_installdeb

debian/libsysemul0.prerm: debian/libsysemul0.prerm.in
	sed 's/@@DEB_HOST_MULTIARCH@@/$(DEB_HOST_MULTIARCH)/g' $< > $@

debian/libsysemul0.postinst: debian/libsysemul0.postinst.in
	sed 's/@@DEB_HOST_MULTIARCH@@/$(DEB_HOST_MULTIARCH)/g' $< > $@
